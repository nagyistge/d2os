<?xml version="1.0"?>
<filter name="compare" file="samples.jar" class="sample.watershed.CompareFilter" instances="4">
	<attr name="k" value="50"/>
	<input>
		<channel name="Samples">
			<deliver class="sample.watershed.LocalLineDeliver" file="samples.jar">
				<attr name="file" value="/home/hadoop/rcor/testing.dat"/>
				<attr name="partition" value="false"/>
			</deliver>
			<decoders>
				<decoder class="sample.watershed.ItemizerDecoder" file="samples.jar" />
			</decoders>
		</channel>
		<channel name="Training">
			<deliver class="sample.watershed.LocalLineDeliver" file="samples.jar">
				<attr name="file" value="/home/hadoop/rcor/training.dat"/>
			</deliver>
			<decoders>
				<decoder class="sample.watershed.ItemizerDecoder" file="samples.jar" />
			</decoders>
		</channel>
	</input>
	<output>
		<channel name="net">
		</channel>
	</output>
</filter>
